<div class= "container mt-5">
<h1>New Task</h1>
<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: "form-control"%>
  <%= f.label :details %>
  <%= f.text_field :details, class: "form-control" %>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
  <div class="mt-2">
  <%= f.submit class: "btn btn-primary"%>
  </div>
<% end %>
</div>
